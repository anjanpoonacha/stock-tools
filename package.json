{
  "name": "mio-tv-scripts",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "analyze-mio": "tsx scripts/analyze-mio-structure.ts",
    "count-mio": "tsx scripts/count-from-html.ts",
    "check-structure": "tsx scripts/check-html-structure.ts",
    "scrape-mio": "tsx scripts/scrape-mio-criteria.ts",
    "package-extension": "node scripts/package-extension.js",
    "version-extension": "node scripts/version-extension.js",
    "release-extension": "pnpm run package-extension && echo 'Extension packaged successfully. Push to main branch to trigger release.'",
    "package-webapp": "node scripts/package-webapp.js",
    "version-webapp": "node scripts/version-webapp.js",
    "release-webapp": "pnpm run package-webapp && echo 'Webapp packaged successfully. Push to main branch to trigger release.'",
    "setup-hooks": "node scripts/setup-hooks.js",
    "postinstall": "node scripts/setup-hooks.js install",
    "poc-test": "tsx scripts/poc-tradingview/poc-test-runner.ts",
    "poc-get-session": "tsx scripts/poc-tradingview/poc-get-session-helper.ts",
    "poc-1": "tsx scripts/poc-tradingview/poc-1-get-user-id.ts",
    "poc-2": "tsx scripts/poc-tradingview/poc-2-get-jwt-token.ts",
    "poc-3": "tsx scripts/poc-tradingview/poc-3-websocket-client.ts",
    "poc-all": "pnpm poc-1 && pnpm poc-2 && pnpm poc-3",
    "test:integration": "tsx --env-file=.env scripts/test-websocket-v2-integration.ts",
    "test:integration:verbose": "DEBUG=* tsx --env-file=.env scripts/test-websocket-v2-integration.ts",
    "test:v2-integration": "tsx --env-file=.env scripts/test-v2-real-integration.ts"
  },
